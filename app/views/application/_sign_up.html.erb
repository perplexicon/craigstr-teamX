<h3>Sign up</h3>
<%= form_for :user do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, placeholder: "Email" %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password, placeholder: "Password" %>
  </div>
  <div>
    <%= form.submit "Sign up" %>
  </div>
<% end %>
